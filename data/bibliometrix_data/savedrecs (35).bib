@article{ WOS:A1995RB12100002,
Author = {BEVANGER, K and LINDSTROM, ER},
Title = {DISTRIBUTIONAL HISTORY OF THE EUROPEAN BADGER MELES-MELES IN SCANDINAVIA
   DURING THE 20TH-CENTURY},
Journal = {ANNALES ZOOLOGICI FENNICI},
Year = {1995},
Volume = {32},
Number = {1},
Pages = {5-9},
Note = {II North European Symposium on the Ecology of Small and Medium-sized
   Carniwores, LAMMI, FINLAND, APR 08-11, 1994},
Abstract = {Early in the 20th century, the badger was permanently established
   throughout southern Scandinavia (except the island of Gotland), and
   scattered populations were thought to occur up to approximately 63
   degrees N (Collet 1911-1912, Ekman 1922). That was also the situation in
   the early 1940's. However, 40 years later individual badgers were
   reported beyond the Arctic Circle up to 67 degrees N in Sweden and 68
   degrees N in Norway (Bevanger 1985), i.e. another 300 km northwards.
   Questionnaires completed by regional and local wildlife boards showed
   that the range of permanent distribution now extends along the Baltic
   coast to the inner part of the Gulf of Bothnia in Sweden, and through
   coastal and inland areas up to 65 degrees N in Norway. Here, we discuss
   the possible causes of this expansion, such as climate, predators and
   human impact.},
