@article{ WOS:A1997WN77300004,
Author = {Wintle, RF and Nygaard, TG and Herbrick, JAS and Kvaloy, K and Cox, DW},
Title = {Genetic polymorphism and recombination in the subtelomeric region of
   chromosome 14q},
Journal = {GENOMICS},
Year = {1997},
Volume = {40},
Number = {3},
Pages = {409-414},
Month = {MAR 15},
Abstract = {Subtelomeric regions of human chromosomes are the sites of increased
   meiotic recombination and have a male-to-female recombination ratio that
   is higher than elsewhere in the genome. We isolated two novel,
   polymorphic CA repeat markers from the distal part of the immunoglobulin
   heavy chain gene cluster, approximately 90 and 200 kb hom the telomere
   of chromosome 14q. The 14q telomere was unambiguously located by
   physical mapping of telomeric YACs and Bal31 exonuclease digestion of
   genomic DNA We then constructed haplotypes using genotype data from
   these markers and data from sCAW1 (D14S826) for use as a highly
   polymorphic genetic marker. Linkage analysis using the 40 pedigree CEPH
   reference panel and genotype data from these and other loci physically
   mapped to the terminal 1.5 Rib of chromosome 14q revealed an apparent
   increase in meiotic recombination within this region, relative to the
   average rate for the genome. Further, we found that recombination was
   higher in females than in males, indicating that the subtelomeric region
   of 14q differs from other human subtelomeric regions. (C) 1997 Academic
   Press.},
