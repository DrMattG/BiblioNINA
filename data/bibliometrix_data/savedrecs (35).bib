@article{ WOS:A1995QD14300046,
Author = {SAVINOVA, TN and POLDER, A and GABRIELSEN, GW and SKAARE, JU},
Title = {CHLORINATED HYDROCARBONS IN SEABIRDS FROM THE BARENTS SEA AREA},
