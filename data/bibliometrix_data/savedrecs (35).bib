Title = {DISTRIBUTION OF ADULT RECAPTURES FROM HATCHERY-REARED ATLANTIC SALMON
   (SALMO-SALAR) SMOLTS RELEASED IN AND OFFSHORE OF THE RIVER SURNA,
   WESTERN NORWAY},
Journal = {AQUACULTURE},
Year = {1991},
Volume = {98},
Number = {1-3},
Pages = {89-96},
Month = {OCT 15},
Abstract = {The hypothesis that motivation of adult salmon to enter fresh water is
   dependent on the location of release site was tested. Three groups of
   smolts were released: into a river; into a fjord outside the river; and
   into the open sea. Numbers and percentages of adult salmon caught in the
   commercial marine fisheries were highest for smolts released from sites
   outside the river. Adults from smolts released in the river did not
   migrate farther upstream than adults released from marine sites. Adults
   homed to the area of smolt release, whether or not the fish were
   released in sea or fresh water. The results also showed that both
   recapture rates and straying increased with increasing release distance
   from fresh water.
   Consequently, both rates of recapture and straying of ocean ranched
   Atlantic salmon are strongly dependent on release location.},
