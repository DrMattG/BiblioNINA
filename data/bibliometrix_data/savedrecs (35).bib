@article{ WOS:A1997XQ03900008,
Author = {Thorstad, EB and Finstad, B and Okland, F and McKinley, RS and Booth, RK},
Title = {Endurance of farmed and sea-ranched Atlantic salmon Salmo salar L at
   spawning},
