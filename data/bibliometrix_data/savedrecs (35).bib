Language = {English},
Affiliation = {ANDERSEN, R (Corresponding Author), NORWEGIAN INST NAT RES,MAMMALIAN ECOL RES GRP,TUNGASLETTA 2,N-7004 TRONDHEIM,NORWAY.},
DOI = {10.2307/1940760},
