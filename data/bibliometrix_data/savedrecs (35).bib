@article{ WOS:A1997WK95900015,
Author = {Hansen, LP and Reddin, DG and Lund, RA},
Title = {The incidence of reared Atlantic salmon (Salmo salar L) of fish farm
   origin at West Greenland},
Journal = {ICES JOURNAL OF MARINE SCIENCE},
Year = {1997},
Volume = {54},
Number = {1},
Pages = {152-155},
Month = {FEB},
Abstract = {The proportion of escaped farmed Atlantic salmon in the commercial
   fishery at West Greenland was estimated using samples of fish caught
   with drift-nets in the commercial fishery in 1991 and 1992.
   Identification of reared fish was carried out using scale analysis.
   Despite the fact that large numbers of salmon escape from fish farms
   around the north Atlantic, the proportion of farmed fish in this fishery
   was low, 1.1\% in 1991 and 1.4\% in 1992, and much lower than in the
   area of the Faroes fishery in the east Atlantic. The low number of fish
   farm escapees at Greenland is surprising since large numbers of farmed
   salmon are produced in countries from which most of the wild salmon that
   occur at West Greenland originate. (C) 1997 International Council for
   the Exploration of the Sea.},
