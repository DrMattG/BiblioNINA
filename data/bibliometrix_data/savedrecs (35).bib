@article{ WOS:A1991FG67600003,
Author = {JENSEN, AJ and JOHNSEN, BO and HEGGBERGET, TG},
Title = {INITIAL FEEDING TIME OF ATLANTIC SALMON, SALMO-SALAR, ALEVINS COMPARED
   TO RIVER FLOW AND WATER TEMPERATURE IN NORWEGIAN STREAMS},
Journal = {ENVIRONMENTAL BIOLOGY OF FISHES},
Year = {1991},
Volume = {30},
Number = {4},
Pages = {379-385},
Month = {APR},
Abstract = {The time of initial feeding of Atlantic salmon, Salmo salar, alevins in
   ten geographically widespread Norwegian streams was estimated
   theoretically by combining data on spawning time and models describing
   the time from fertilization to hatching and from hatching to initial
   feeding at different temperatures.  The point of initial feeding was
   correlated with water flow and water temperature regimes.  Initial
   feeding was avoided in all rivers during spring peak flow, and before
   water temperature reached 8-degrees-C.  Two different strategies were
   indicated:  (1) initial feeding may take place before the culmination of
   the spring flow or (2) after the peak spring flow.  The choice of
   strategy depends on temperature and flow regimes in each river.},
