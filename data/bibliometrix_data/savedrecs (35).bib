@article{ WOS:A1993MU26300010,
Author = {HOILAND, K and DYBDAHL, HG},
Title = {A MICRO-WELL METHOD FOR ESTIMATING FUNGAL RESPONSE TO METAL-IONS -
   RESPONSE TO ALUMINUM BY SOME SAPROPHYTIC BASIDIOMYCETES},
Journal = {NORDIC JOURNAL OF BOTANY},
Year = {1993},
Volume = {13},
Number = {6},
Pages = {691-696},
Abstract = {Cultures of 19 saprophytic basidiomycete species were examined for
   aluminium resistance using a micro-well technique. The following
   aluminium concentrations have been employed: 0, 1, e1, e2, e3, and e4 mM
   given as sulphate. Three of the saprophytic species were also tested at
   the pH values generated by the same Al concentrations. The following
   species were considered resistant: Mycena galopus, M. metata, M.
   inclinata, M. viridimarginata, Marasmius androsaceus, Galerina
   calyptrata, and Hypholoma capnoides. The following were sensitive:
   Micromphale perforans, Mycena cinerella, Clitocybe fragrans, and
   Clitocybe metachroa. The remaining species showed intermediate
   responses. The autecology of species exhibiting either a high resistance
   or a high sensitivity is discussed. Mycena galopus, found to be
   particularly resistant, may be specially tolerant to acidification since
   it is reported to increase or persist in polluted environments by
   several authors.},
