@article{ WOS:A1995UB05300059,
Author = {Hesthagen, T and Berger, HM and Larsen, BM and Saksgard, R},
Title = {Monitoring fish stocks in relation to acidification in Norwegian
   watersheds},
Journal = {WATER AIR AND SOIL POLLUTION},
Year = {1995},
