@article{ WOS:000250301900005,
Author = {Johnsen, Stein I. and Taugbol, Trond and Andersen, Oddgeir and Museth,
   Jon and Vralstad, Trude},
Title = {The first record of the non-indigenous signal crayfish Pasifastacus
   leniusculus in Norway},
Journal = {BIOLOGICAL INVASIONS},
Year = {2007},
Volume = {9},
Number = {8},
Pages = {939-941},
Month = {DEC},
Abstract = {The non-indigenous signal crayfish Pasifastacus leniusculus was
   registered for the first time in Norway in October 2006. The location
   represents an isolated pond about 100 km in air line from the nearest
   known signal crayfish population in the neighbouring country Sweden
   without any connecting watercourses. The occurrence is therefore
   undoubtedly caused by human introduction. Molecular analyses confirmed
   that tested individuals from the signal crayfish population were
   carriers of the crayfish plague agent Aphanomyces astaci. Disease
   carrying signal crayfish represents a severe threat to the indigenous
   and endangered noble crayfish Astacus astacus. Norwegian authorities are
   currently considering actions for the eradication of the signal crayfish
   population.},
