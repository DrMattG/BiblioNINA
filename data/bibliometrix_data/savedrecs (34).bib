@article{ WOS:000233786300010,
Author = {Skuterud, L and Gwynn, JP and Gaare, E and Steinnes, E and Hove, K},
Title = {Sr-90, Po-210 and Pb-210 in lichen and reindeer in Norway},
Journal = {JOURNAL OF ENVIRONMENTAL RADIOACTIVITY},
Year = {2005},
Volume = {84},
Number = {3},
Pages = {441-456},
Abstract = {Concentrations of Sr-90, Po-210 and Pb-210 in lichen and reindeer were
   studied in central (Ostre Namdal) and southern Norway (Vaga) during
   2000-2003. The study focussed on potential differences in concentrations
   of these nuclides in reindeer of different ages. Concentrations of Sr-90
   in bones of similar to 10 year old adult females were about 40\% higher
   than those in calves' antlers (Sr-90 concentrations in antlers and bones
   of calves are similar), while the available data from Vaga suggest that
   Sr-90 concentrations in reindeer calves decreased with an effective
   ecological half-time of 9.03 +/- 0.06 years during 1988-2002.
   Furthermore, Sr-90 concentrations were 50-80\% higher in bone of
   reindeer of a similar age from Vaga compared to those from Ostre Namdal.
   Concentrations of Po-210 and Pb-210 in muscle and liver tissues were
   comparable to those reported for reindeer in other Nordic areas, with no
   significant difference in Po-210 and Pb-210 concentrations between
   adults and calves or between reindeer from the two different study
   areas. (c) 2005 Elsevier Ltd. All rights reserved.},
Publisher = {ELSEVIER SCI LTD},
Address = {THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, OXON, ENGLAND},
Type = {Article},
Language = {English},
