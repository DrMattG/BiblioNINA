@article{ WOS:000177002400025,
Author = {Strand, R and Finstad, B and Lamberg, A and Heggberget, TG},
Title = {The effect of Carlin tags on survival and growth of anadromous Arctic
   charr, Salvelinus alpinus},
Journal = {ENVIRONMENTAL BIOLOGY OF FISHES},
Year = {2002},
Volume = {64},
Number = {1-3},
Pages = {275-280},
Month = {JUN},
Note = {4th International Charr Symposium, TROIS RIVIERES, CANADA, JUN 25-JUL
   01, 2000},
Abstract = {Fish tagging methods are important tools in the research and management
   of salmonids; the Carlin tag is widely used, especially in anadromous
   salmonids. However, this tagging method may interfere with fish
   behaviour and increase post-smolt mortality. To estimate
   Carlin-tag-inflicted mortality in Arctic charr, We used jet injection of
   alcian blue (colour tags) as a control. Recapture rates were higher for
   the colour-tagged fish than for the Carlin-tagged fish for both one- and
   two-year-old smolts. For one-year-old smolts in 1992, the recapture rate
   was 18.9\% for colour-tagged fish and 10.0\% for Carlin-tagged ones. For
   two-year-old smolts in the same year, 34.0\% of colour- and 28.6\% of
   Carlin-tagged fish were recaptured; in 1993, recapture rates were 17.2\%
   and 6.3\% for colour- and Carlin-tagged fish, respectively.
   Colour-tagged one-year-old Arctic charr also grew faster than their
   Carlin-tagged counterparts. No differences in growth rates were found
   between tagging methods in two-year-olds. These results imply that for
   hatchery-reared Arctic charr, survival rates using Carlin tagging may be
   half to one third that of colour tagging. Growth was also higher among
   colour-tagged fish, indicating that Carlin tags alter the behaviour of
   fish in the sea.},
Publisher = {KLUWER ACADEMIC PUBL},
