@article{ WOS:000085653000002,
Author = {Bustnes, JO and Asheim, M and Bjorn, TH and Gabrielsen, H and Systad, GH},
Title = {The diet of Steller's Eiders wintering in Varangerfjord, northern Norway},
Journal = {WILSON BULLETIN},
Year = {2000},
Volume = {112},
Number = {1},
Pages = {8-13},
Month = {MAR},
Abstract = {We examined the winter diet of Stellar's Elders (Polysticta stelleri) in
   Varangerfjord, northern Norway, by analyzing the esophagus content of 29
   individuals (12 juveniles and 17 adults). A total of 8389 prey items of
   31 species were identified: 13 species of gastropods (making up 68.4\%
   of total number of items); 4 species of bivalves (18.5\%); 12 species of
   crustaceans (13\%); and 2 species of echinoderms (<0.1\%). In terms of
   percentage aggregate wet weight 31.4\% was gastropods, 22.6\% was
   bivalves, and 41.4\% was crustaceans. Juvenile elders ate more
   crustaceans ((x) over bar = 61\% aggregate w.w.) than adults ((x) over
   bar = 26\%, P < 0.05), possibly because they were in poor body condition
   and may have had higher energy requirements than adults. Adults tended
   to eat more gastropods ((x) over bar = 41\% vs 22\%) and Mytilus edulis
   ((x) over bar = 27\% vs 12\%) than juveniles. There were only small
   differences between sexes. Most of the prey items were of species known
   to be associated with kelp plants, especially Laminaria hyperborea,
   suggesting that Steller's Elders obtain a large proportion of their prey
   directly from the vegetation.},
