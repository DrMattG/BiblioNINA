Affiliation = {Tombre, IM (Corresponding Author), Norwegian Inst Nat Res, Polar Environm Ctr, Div Arctic Ecol, N-9296 Tromso, Norway.
   Norwegian Inst Nat Res, Polar Environm Ctr, Div Arctic Ecol, N-9296 Tromso, Norway.
   Natl Environm Res Inst, Dept Arctic Environm, DK-4000 Roskilde, Denmark.
   Sortland Municipal, Dept Agr, N-8400 Sortland, Norway.
   Norut NIBR Finnmark, N-9505 Alta, Norway.},
DOI = {10.1016/j.agee.2005.06.007},
ISSN = {0167-8809},
EISSN = {1873-2305},
Keywords = {habitat choice; agricultural conflicts; scaring campaigns; pink-footed
