@article{ WOS:000235845500011,
Author = {Persson, J and Landa, A and Andersen, R and Segerstrom, P},
Title = {Reproductive characteristics of female wolverines (Gulo gulo) in
   Scandinavia},
Journal = {JOURNAL OF MAMMALOGY},
Year = {2006},
Volume = {87},
Number = {1},
Pages = {75-79},
Month = {FEB},
Abstract = {We Studied reproduction of 56 female wolverines (Gulo gulo) in 2 areas
   of northern Scandinavia. Minimum average age at 1st reproduction was 3.4
   years. Mean proportion of females (>= 3 years old) reproducing was 0.53
   (95\% confidence interval {[}CI] = 0.43-0.63, n = 94), and the annual
   mean was 0.58 (95\% Cl = 0.35-0.80, n = 83). Mean annual birth rate was
   0.74 (95\% Cl = 0.33-1.14, n = 83) young per female (>= 3 years old).
   Mean size of 74 litters was 1.88 (95\% Cl = 1.68-2.07, range = 1-4).
   Examination Of Our data Suggests that female wolverines have low
