@article{ WOS:000222289600010,
Author = {Wagner, GN and McKinley, RS and Bjorn, PA and Finstad, B},
Title = {Short-term freshwater exposure benefits sea lice-infected Atlantic
   salmon},
Journal = {JOURNAL OF FISH BIOLOGY},
Year = {2004},
Volume = {64},
Number = {6},
Pages = {1593-1604},
Month = {JUN},
Abstract = {Atlantic salmon Salmo salar were infected with sea lice Lepeoplitheirus
   salmonis (0(.)08 +/- 0(.)007 sea lice g(-1)) over a period of 4 h. Both
   infected and non-infected fish were swim tested in sea water (SW) and
   fresh water (FW). The ventral aorta of each fish was fitted with a
   Doppler cuff in order to measure cardiac output, stroke volume and heart
   rate during swim testing. Blood samples were taken at rest and after
   exercise. Critical swimming speed of infected fish in SW (2(.)14 +/-
   0.08 body lengths, bls(-1)) was significantly lower (P<0.05) than
   infected fish switched to FW (2.81 +/- 0-08 bls(-1)) and non-infected
   fish in SW (2(.)42 +/- 0.04 bls(-1)) and FW (2(.)61 +/- 0(.)08 bls(-1)).
   Cardiac and blood results indicated infected fish exposed to FW did
