   Nellemann, Christian, Norwegian Inst Nat Res, N-2624 Stohove, Lillechammer, Norway.},
DOI = {10.1007/s00300-007-0377-9},
ISSN = {0722-4060},
EISSN = {1432-2056},
Keywords = {caribou; disturbance; oil; pipelines; reindeer; roads; scale},
Keywords-Plus = {TRADITIONAL ECOLOGICAL KNOWLEDGE; FLIGHT INITIATION DISTANCE; ALASKA
