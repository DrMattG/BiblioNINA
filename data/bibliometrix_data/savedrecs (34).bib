@article{ WOS:000284550100007,
Author = {Skar, Margrete and Krogh, Erling},
Title = {Changes in children's nature-based experiences near home: from
   spontaneous play to adult-controlled, planned and organised activities},
Journal = {CHILDRENS GEOGRAPHIES},
Year = {2009},
Volume = {7},
Number = {3},
Pages = {339-354},
Abstract = {A qualitative case study from Brumunddalen, Norway, indicates that
   children's use of nature areas near their homes have changed
   substantially from being spontaneous and self-initiated to being part of
   planned, organised and adult-controlled activities. This study explores
   the association between changes in outdoor play and wider patterns of
   change in childhood today, as increasing amounts of play time is spent
   in institutions and organised activities, or occupied by computer and
   internet usage. The paper discusses these effects in terms of social
   relations as well as children's identity formation. The subject was
   investigated through adults' memories of their own childhood and their
   observations of children's practices.},
