@article{ WOS:000172776000010,
Author = {Dramstad, WE and Fry, G and Fjellstad, WJ and Skar, B and Helliksen, W
   and Sollund, MLB and Tveit, MS and Geelmuyden, AK and Framstad, E},
Title = {Integrating landscape-based values - Norwegian monitoring of
   agricultural landscapes},
Journal = {LANDSCAPE AND URBAN PLANNING},
Year = {2001},
Volume = {57},
Number = {3-4},
Pages = {257-268},
