@article{ WOS:000227337200010,
Author = {Finstad, AG and Ugedal, O and Forseth, T and Naesje, TF},
Title = {Energy-related juvenile winter mortality in a northern population of
   Atlantic salmon (Salmo salar)},
Journal = {CANADIAN JOURNAL OF FISHERIES AND AQUATIC SCIENCES},
Year = {2004},
Volume = {61},
Number = {12},
Pages = {2358-2368},
Month = {DEC},
Note = {Symposium on Status of Atlantic Salmon, Quebec, CANADA, AUG 11-14, 2003},
Abstract = {By comparing the population frequency distributions for specific somatic
   energy between samplings using quantile-quantile (QQ) plots, we tested
   for energy-related mortality of juvenile (2- and 3-year-old) Atlantic
   salmon (Salmo salar) sampled at monthly intervals throughout three
   consecutive winters in a Norwegian river located at 70degreesN. Between
   several of the sampling periods, changes in the distributions of
   specific energy were observed corresponding to removal of low-energy
   individuals. By using energetic modelling we demonstrated that metabolic
   processes or feeding could not be responsible for the shifts in the
   shape of the energy distributions and that negative-energy-dependent
   mortality was the most likely explanation for the observations. No
   changes in mean size of the fish or in the shape of the size
   distributions were observed between successive sampling periods,
   indicating that mortality was linked to levels of storage energy rather
   than to body size per se. Our study indicated a critical body energy
   level for survival of juvenile salmon at approximately 4400-4800
   J(.)g(-1), corresponding to a depletion of storage lipids.},
