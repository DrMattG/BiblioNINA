@article{ WOS:000182383300042,
Author = {Iversen, M and Finstad, B and McKinley, RS and Eliassen, RA},
Title = {The efficacy of metomidate, clove oil, Aqui-S (TM) and Benzoak (R) as
   anaesthetics in Atlantic salmon (Salmo salar L.) smolts, and their
   potential stress-reducing capacity},
Journal = {AQUACULTURE},
Year = {2003},
Volume = {221},
Number = {1-4},
Pages = {549-566},
Month = {MAY 1},
Abstract = {Atlantic salmon (Salmo salar L.) smolts (45 g) in freshwater (5.4
   degreesC) were exposed to water containing different concentrations of
   metomidate from 0 to 10 mg l(-1), clove oil (eugenol), Aqui-S(TM)
   (iso-eugenol) and Benzoak(R) (benzocaine) from 0 to 100 mg l(-1). All
   anaesthetic concentrations were mixed and expressed as mg l(-1) in
   relation to the active substance of the various anaesthetics.
   Metomidate, clove oil, Aqui-S(TM) and Benzoak(R) were effective as
   anaesthetics for Atlantic salmon smolts at concentrations : greater than
   or equal to 2 mg l(-1) (metomidate) and greater than or equal to 30 mg
   l(-1) (clove oil, Aqui-S(R) , Benzoak(R)). The fish entered behavioural
   stage 4 (failure to respond to external stimuli) within 6.2-2.2 min for
   metomidate concentrations from 2 to 10 mg l(-1), within 8.1-2.2 min
   (clove oil and Aqui-S(TM)) and 17.4-3.0 min (Benzoak(R)) for
   concentrations from 30 to 100 mg l(-1). A concentration of 100 mg l(-1)
   of Benzoak(R), clove oil and Aqui-S(TM) caused the salmon smolts to
   enter stage 5 (arrested opercular activity) after 6.4 and 12 min of
   exposure, respectively Metomidate solutions greater than or equal to 2
   mg l(-1), clove oil and Aqui-S(TM) solutions greater than or equal to 20
   mg l(-1) prevented plasma cortisol elevation above resting level.
   Benzoak(R) (regardless of dosage) did not prevent plasma cortisol
   elevations above resting level. While the anaesthetics increased mean
   plasma lactate concentration significantly in nearly all experimental
   groups, no such changes in plasma glucose were evident in any of the
   experimental groups. The eugenol-based anaesthetics (clove oil and
   Aqui-S(TM)) show promise to become effective anaesthetics, with good
   efficacy at low dosages and with stress-reducing capabilities. They are
   also inexpensive and easily obtained, and are organic substances safe
   for both environment and user. (C) 2003 Published by Elsevier Science
   B.V.},
