@article{ WOS:000231044200008,
Author = {Skuterud, L and Gaare, E and Eikelmann, IM and Hove, K and Steinnes, E},
Title = {Chernobyl radioactivity persists in reindeer},
Journal = {JOURNAL OF ENVIRONMENTAL RADIOACTIVITY},
Year = {2005},
Volume = {83},
Number = {2},
Pages = {231-252},
Abstract = {Transfer of Cs-137 in the soil-plant/lichen-reindeer food chain was
   studied in central (Ostre Namdal) and southern Nor-way (Vaga) during
   2000-2003. Reindeer from these areas have been continuously subjected to
   countermeasure application since the 1986 Chernobyl accident. In both
   areas no decline in Cs-137 concentrations was detectable in reindeer
   slaughtered in autumn since 1995, or in reindeer slaughtered in winter
   since 1998-1999. Seasonal differences in Cs-137 concentrations in
   reindeer have been less pronounced in recent years, with Cs-137
   concentrations occasionally higher in autumn than in winter.
   Soil-to-plant Cs-137 transfer was significantly higher in Ostre Namdal
   than in Vaga. Climatic influences on lichen growth and abundance, and on
   soil properties that influence the availability of Cs-137 for plant
   uptake, are hypothesized to have a larger impact on long-term transfer
   of radiocaesium in the soil-plant/lichen-reindeer food chain than has
   been previously observed. (c) 2005 Elsevier Ltd. All rights reserved.},
