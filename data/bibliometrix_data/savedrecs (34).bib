@article{ WOS:000073611000023,
Author = {Soleng, A and Bakke, TA and Hansen, LP},
Title = {Potential for dispersal of Gyrodactylus salaris (Platyhelminthes,
   Monogenea) by sea-running stages of the Atlantic salmon (Salmo salar):
   field and laboratory studies},
Journal = {CANADIAN JOURNAL OF FISHERIES AND AQUATIC SCIENCES},
Year = {1998},
Volume = {55},
Number = {2},
Pages = {507-514},
Month = {FEB},
Abstract = {Population growth of Gyrodactylus salaris increased exponentially on
   Atlantic salmon (Salmo salar) smelts in laboratory experiments conducted
   at 12.0 degrees C. Furthermore, G. safaris was transmitted successfully
   from salmon smelt to parr at 0.0, 7.5, 10.0, and 20.0 parts per thousand
   salinity and reproduced in fresh water after direct transfer from 7.5
   parts per thousand (16 days), 20.0 parts per thousand (4 and 8 h), and
   33.0 parts per thousand (5, 15, and 30 min). No G. salaris were observed
   on salmon parr exposed to 33.0 parts per thousand for 60 min. The
   prevalence of G. salaris on wild salmon smelts caught approximately 25
   km from the river mouth in the Drammensfjord (surface salinity 2.0-3.5
   parts per thousand) was 71.2\% compared with 88.0\% on those from the
   neighbouring River Lierelva. Adult wild salmon caught as prespawners,
   spawners, and postspawners (kelts) in the River Drammenselva were
   infected with G. salaris. The prevalence and abundance increased from
   autumn to spring, in contrast with earlier studies on salmon parr,
   demonstrating the possible importance of adult salmon as reservoirs for
   G. safaris during winter. The results support the hypothesis of brackish
   water dispersal of G. safaris by infected salmonids migrating in
   estuaries and fjords. The use of salt as a disinfectant against G.
   safaris in hatcheries, and the stocking of possibly infected fish into
   brackish and seawater, should also be reexamined.},
