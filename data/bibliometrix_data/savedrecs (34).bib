@article{ WOS:000239917100003,
Author = {Fiske, Peder and Lund, Roar A. and Hansen, Lars P.},
Title = {Relationships between the frequency of farmed Atlantic salmon, Salmo
   salar L., in wild salmon populations and fish farming activity in
   Norway, 1989-2004},
Journal = {ICES JOURNAL OF MARINE SCIENCE},
Year = {2006},
Volume = {63},
Number = {7},
Pages = {1182-1189},
Month = {AUG},
Note = {ICES/NASCO Symposium on Interactions between Aquaculture and Wild Stocks
   of Atlantic Salmon and other Diadromous Fish Species, Bergen, NORWAY,
   OCT 18-21, 2005},
Organization = {ICES; NASCO},
Abstract = {In Norway, there have been restrictions on salmon farming in several
   fjords to reduce the potential negative impact on important stocks of
   wild Atlantic salmon. Little is known about the incidence of escaped
   fanned salmon in fisheries and broodstocks relative to the extent of
   fish fanning in nearby areas. In this study, we analysed data on the
   incidence of escaped farmed Atlantic salmon in angling catches and
   broodstock fisheries in rivers for a 16-year period (1989-2004). These
   data were weighted using official catch statistics and combined at the
   county level, and the incidence of escapees was correlated with both the
   stock of farmed salmon in net pens and the reported number of escapees
   in different Norwegian counties. Our results indicate a significant
   positive correlation between the incidence of escaped farmed salmon in
   the rivers at the county level and the intensity of salmon fanning,
   measured as the number of fanned salmon in net pens, suggesting that
   protection areas may reduce the impact of escapees in salmon populations
   nearby. (c) 2006 International Council for the Exploration of the Sea.
