@article{ WOS:000174440400009,
Author = {Bustnes, JO and Erikstad, KE and Bjorn, TH},
Title = {Body condition and brood abandonment in common eiders breeding in the
   high Arctic},
Journal = {WATERBIRDS},
Year = {2002},
