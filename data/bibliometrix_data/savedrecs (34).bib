@article{ WOS:000170122100008,
Author = {Vistnes, I and Nellemann, C and Jordhoy, P and Strand, O},
Title = {Wild reindeer: impacts of progressive infrastructure development on
   distribution and range use},
Journal = {POLAR BIOLOGY},
Year = {2001},
Volume = {24},
Number = {7},
Pages = {531-537},
Month = {JUL},
Abstract = {We investigated the distribution of wild reindeer (Rangifer tarandus
   tarandus) from aerial surveys in 1986-1998 in 622 contiguous 4-km(2)
   grid squares in relation to density of power lines, roads and ski trails
   in the Nordfjella region, south-central Norway. Density of reindeer was
   significantly lower in developed quadrats compared to undeveloped
   quadrats and decreased with increasing density of development. No
   reindeer were observed in areas exceeding 1.3 km/km(2) of linear
   structures and only 1.1\% of the reindeer were observed in areas
   exceeding 0.8 km/km(2) of linear structures, an area constituting 17\%
   of the study area. The results imply that development, and also the
   degree of development, affect the availability of range for wild
   reindeer. Furthermore. the results suggest that reindeer may abandon
   areas once disturbance exceeds a critical level. We conclude that there
   is a need for cumulative impact assessments of the overall development
   in Norwegian mountain regions, to preserve the last remaining wild
   reindeer populations in Europe.},
