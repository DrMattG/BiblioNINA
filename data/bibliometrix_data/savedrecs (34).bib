@article{ WOS:000077308400033,
Author = {Ugedal, O and Finstad, B and Damsgard, B and Mortensen, A},
Title = {Seawater tolerance and downstream migration in hatchery-reared and wild
   brown trout},
Journal = {AQUACULTURE},
Year = {1998},
Volume = {168},
Number = {1-4},
Pages = {395-405},
Month = {OCT 1},
Note = {5th International Workshop on Salmonnid Smoltification, MUONIO, FINLAND,
   DEC 09-12, 1996},
Abstract = {Seawater tolerance and downstream migration were examined in 2-year old
   hatchery-reared brown trout, Salmo trutta L., and compared with wild
   downstream migrating trout in the spring 1994 and 1995, using a fish
   trap in the River Halselva, northern Norway. At release, seawater
   challenge tests (72 h, 34 parts per thousand) showed that about half of
   the variance in seawater tolerance of hatchery-reared fish was explained
   by fish size. On average, 34\% of the released trout migrated
   downstream, whereas 44-51\% had acceptable seawater tolerance at release
   (seawater challenge plasma chloride less than or equal to 160 mmol
   l(-1)). The migration tendency in hatchery-reared fish increased with
   increasing fish size at release. Migrating hatchery-reared fishes and
   first-time migrants of wild trout showed a well-developed
   hypoosmoregulatory capacity. Downstream migration in this brown trout
   stock therefore appears to be associated with a well-developed seawater
   tolerance. (C) 1998 Elsevier Science B.V. All rights reserved.},
