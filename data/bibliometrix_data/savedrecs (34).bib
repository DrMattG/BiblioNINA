@article{ WOS:000259193900016,
Author = {Guenard, Guillaume and Boisclair, Daniel and Ugedal, Ola and Forseth,
   Torbjorn and Jonsson, Bror},
Title = {Comparison between activity estimates obtained using bioenergetic and
   behavioural analyses},
Journal = {CANADIAN JOURNAL OF FISHERIES AND AQUATIC SCIENCES},
Year = {2008},
Volume = {65},
Number = {8},
Pages = {1705-1720},
Month = {AUG},
Abstract = {Activity rate of Arctic char (Salvelinus alpinus) held in 90 m 2
   littoral enclosures were estimated using bioenergetic (with consumption
   estimated using stable caesium, Cs-133) and behavioural approaches (with
   fish movements quantified using video cameras). We found no
   statistically significant difference between Values of activity rate
   obtained using the two approaches for three of the six experiments we
   performed. However, there was no relationship between estimates of
   activity rate obtained using the two approaches. Discrepancies may arise
   from the difficulty to meet assumptions regarding (lie temporal
   stability of the concentration of Cs-113 in fish diet and of the
   assimilation coefficient of this tracer. When fish remain in all area
   where their behaviour call be well described (e.g., enclosure, habitat
   patches of littoral zones, coral reefs). the behavioural approach
   appears more robust to estimate activity rate because it depends most on
   a variable that is easiest to estimate (the number of movements
   performed). When these conditions are not met (low fish densities or
   major fish migration,), a reliable assessment of the concentration and
   assimilation of Cs-133 in stomach contents appears critical to implement
   the bioenergetic approach based on this tracer.},
